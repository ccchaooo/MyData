--查询被锁的对象
SELECT OBJECT_NAME,MACHINE,S.SID,S.SERIAL#, 'alter system kill session '''||S.SID||','||S.SERIAL#||''';'
  FROM V$LOCKED_OBJECT L,DBA_OBJECTS O ,V$SESSION S
 WHERE L.OBJECT_ID = O.OBJECT_ID
   AND L.SESSION_ID = S.SID;
   
--用于测试被修改的数据是否正常备份
SELECT CZY.C_YWMC,DAT.N_CZRZBH,CZY.D_CZSJ,DAT.C_TABLE,DAT.C_COLUMN,DAT.C_OLDVALUES,DAT.C_NEWVALUES,
' SELECT * FROM ' || DAT.C_TABLE || ' WHERE '|| DAT.C_TABLEKEY || ' = ' || DAT.C_KEYVALUE
FROM
 PLATE_LOG_CZY CZY ,
 PLATE_LOG_DATA DAT
 WHERE CZY.C_YYFWDM = DAT.C_YYFWDM
 AND CZY.N_CZRZBH = DAT.N_CZRZBH
 -- AND CZY.N_CZRZBH >1475
 AND CZY.D_CZSJ = (SELECT MAX(A.D_CZSJ) FROM PLATE_LOG_CZY A)
 ORDER BY N_CZRZBH DESC

--招标核准备份表
SELECT * FROM T_JSXM_BDHZ_INFO_BAK ORDER BY N_BDID;

--标段表
SELECT * FROM T_JSXM_BDXX_INFO WHERE n_BDID = 1006272

